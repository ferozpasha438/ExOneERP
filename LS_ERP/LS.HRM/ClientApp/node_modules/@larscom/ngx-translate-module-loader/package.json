{
  "name": "@larscom/ngx-translate-module-loader",
  "version": "3.0.5",
  "description": "Highly configurable and flexible translations loader for ngx-translate. Fetch multiple translations, each translation file gets it's own namespace by default",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larscom/ngx-translate-module-loader"
  },
  "keywords": [
    "angular",
    "ngx-translate",
    "translate",
    "rxjs",
    "module",
    "http",
    "i18n"
  ],
  "author": "Lars Kniep",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larscom/ngx-translate-module-loader/issues"
  },
  "homepage": "https://github.com/larscom/ngx-translate-module-loader#readme",
  "peerDependencies": {
    "@angular/core": ">=6.0.0",
    "@angular/common": ">=6.0.0",
    "@ngx-translate/core": ">=10.0.0",
    "rxjs": ">=6.0.0"
  },
  "dependencies": {
    "deepmerge": ">=3.0.0",
    "tslib": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "fesm2015/larscom-ngx-translate-module-loader.mjs",
  "es2020": "fesm2020/larscom-ngx-translate-module-loader.mjs",
  "esm2020": "esm2020/larscom-ngx-translate-module-loader.mjs",
  "fesm2020": "fesm2020/larscom-ngx-translate-module-loader.mjs",
  "fesm2015": "fesm2015/larscom-ngx-translate-module-loader.mjs",
  "typings": "larscom-ngx-translate-module-loader.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./larscom-ngx-translate-module-loader.d.ts",
      "esm2020": "./esm2020/larscom-ngx-translate-module-loader.mjs",
      "es2020": "./fesm2020/larscom-ngx-translate-module-loader.mjs",
      "es2015": "./fesm2015/larscom-ngx-translate-module-loader.mjs",
      "node": "./fesm2015/larscom-ngx-translate-module-loader.mjs",
      "default": "./fesm2020/larscom-ngx-translate-module-loader.mjs"
    }
  },
  "sideEffects": false
}